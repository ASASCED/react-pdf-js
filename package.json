{
  "name": "@mikecousins/react-pdf",
  "version": "5.2.0",
  "description": "Simple React component to wrap up PDF.js. The easiest way to render PDFs in your React app.",
  "author": "mikecousins",
  "license": "MIT",
  "repository": "mikecousins/react-pdf-js",
  "main": "dist/index.js",
  "module": "dist/react-pdf.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint",
    "netlify": "yarn && yarn build && cd example && yarn && yarn build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "husky": "3.0.5",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "tsdx": "0.9.2",
    "tslib": "1.10.0",
    "typescript": "3.6.3",
    "webpack": "4.40.0"
  },
  "dependencies": {
    "@bundled-es-modules/pdfjs-dist": "^2.1.266",
    "pdfjs-dist": "2.2.228"
  }
}
