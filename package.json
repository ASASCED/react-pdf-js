  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "husky": "^2.2.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.5.7",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "webpack": "^3.0.0 || ^4.0.0-alpha.0 || ^4.0.0"
  },
  "dependencies": {
    "pdfjs-dist": "2.1.266"
  }
